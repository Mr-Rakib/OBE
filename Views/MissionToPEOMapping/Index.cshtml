@model IEnumerable<OBETools.Models.MissionToPEOMapping>

    <div>
        <div class="pt-2">
            @Html.Partial("Notifications/_Notification")
        </div>
        <div class="row">
            <h2 class="mt-4 mb-1 font-weight-light"><i class="fas fa-map-signs p-1"></i>&nbsp; Mission & PEO (Mapping)</h2>
            <hr />

        </div>
        <hr class="c-hr-gray" />
        <!-------------------TAB------------------>
        <ul class="nav nav-tabs mb-1" id="myTab" role="tablist">

            <li class="nav-item">
                <a class="nav-link active" id="users-tab" data-toggle="tab" href="#user" role="tab" aria-controls="user" aria-selected="true">Mission to PEO</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="vets-tab" data-toggle="tab" href="#vet" role="tab" aria-controls="vet" aria-selected="false">PEO to PLO</a>
            </li>
        </ul>
        <div class="tab-content" id="myTabContent">

            <div class="tab-pane fade show active" id="user" role="tabpanel" aria-labelledby="user-tab">
                @Html.Partial("Partial/MissionToPEO")
            </div>
            <div class="tab-pane fade" id="vet" role="tabpanel" aria-labelledby="vet-tab">
                @Html.Partial("Partial/MissionToPEO")
            </div>
        </div>
        <!-------------------TAB------------------>

    </div>


<!--Profile Modal-->
<div class="modal fade" id="view-mission-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewmissionmodal">

    </div>
</div>

<div class="modal fade" id="view-peo-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewpeomodal">

    </div>
</div>


<div class="modal fade" id="view-plo-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewplomodal">

    </div>
</div>

